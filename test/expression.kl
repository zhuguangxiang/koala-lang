
package test;

import "koala/io";

func Main(args []string) {
   var a = 100;
   var b = 12;
   var c = a - b;
   var d = a * b;
   var e = a / b;
   var f = a % b;
   var g = b - a;
   var h = -a;
   io.Println("===================");
   io.Println(a, b, c, d, e, f, g, h);

   var f1 = 2.2;
   var f2 = f1 + 2;
   var f3 = f1 - 2;
   var f4 = f1 * 2;
   var f5 = f1 / 2;
   var f6 = -f1;
   io.Println("===================");
   io.Println(f1, f2, f3, f4, f5, f6);
}