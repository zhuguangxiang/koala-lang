

import "koala/io"

func Main(args:[string]) {
  var arr : [int] = [1, 3, 9, 8, 6, 2]
  var arr2 : [[int]] = [[1,2], [4, 5], [8,10]]
  io.Println(arr)
  io.Println(arr2)

  var arr3 : [string] = ["hello", "world"]
  io.Println(arr3)

  var i:int = arr[1]
  io.Println(i)

  var arr4 = arr2[1]
  io.Println(arr4)

  i = arr2[1][1]
  io.Println(i)

  arr[1] = 100
  io.Println(arr)

  arr2[1][1] = 101
  io.Println(arr2)
}