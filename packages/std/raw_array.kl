@intrinsic
public struct RawArray<T> {

    @intrinsic
    public func __get_item__(idx u32) T

    @intrinsic
    public func __set_item__(idx u32, val T)

    @intrinsic
    public func len() u32

    @intrinsic
    public func as_ptr() Pointer<T>
}

@intrinsic
public func raw_array_new<T>(len uint32) RawArray<T>
